{
    "description": "Many media devices have a navigation pad for browsing a library or a menu. This interface represents a basic navigation pad.",
    "extends": "media",
    "actions": [
        {
            "name": "navigate",
            "params": [
                {
                    "name": "to",
                    "type": "QString",
                    "allowedValues": [
                        "up",
                        "down",
                        "left",
                        "right",
                        "enter",
                        "back"
                    ]
                }
            ]
        }
    ]
}
